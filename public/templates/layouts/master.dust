<!DOCTYPE html>
<html ng-app="QuizApp">
    <head>
        <title>{+title /}</title>
        <!-- for-mobile-apps -->
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
                function hideURLbar(){ window.scrollTo(0,1); } </script>
        <!-- //for-mobile-apps -->
        <link href="/css/fallbacks/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
        <link href="/css/fallbacks/style.css" rel="stylesheet" type="text/css" media="all" />
        <link href="/css/fallbacks/font-awesome.min.css" rel="stylesheet">
        <link rel="stylesheet" href="/css/app.css">
        <!-- js -->
        <script src="/js/fallbacks/jquery-1.11.1.min.js"></script>
        <!-- //js -->

        <link href='/css/fonts/fonts.raleway.css' rel='stylesheet' type='text/css'>
    </head>
    
    <!-- TODO: move csrf to cookie -->
    <body data-csrf="{_csrf}">
        <div id="mainContainer"></div>
        <!-- for bootstrap working -->
        <script src="/js/fallbacks/bootstrap.js"> </script>
        <!-- //for bootstrap working -->
        <!-- webpack generated script -->
        <script src="/js/main.js"></script>
    </body>
</html>